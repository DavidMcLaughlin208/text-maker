<div class="content">

	<h1>Add a new Team</h1>

	<%= form_for @team do |f| %>
		
		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :sport_type %>
		<%= f.text_field :sport_type %>

		<%= f.label :league_name %>
		<%= f.text_field :league_name %>

		<%= f.submit "Submit" %>
	<% end %>

</div>
